chapter1:
  cmp bx,FEA8
  je codea
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2FDA168],00
  je resetscounter
  mov byte ptr [minimumspawn],#1
  mov byte ptr [maximumspawn],#2
  mov byte ptr [chpstage],00
  //1
  cmp bx,D708
  je setenemychp0101
  //2
  cmp bx,FE68
  je setenemychp0102
  //3
  cmp bx,23D0
  je setenemychp0103
  //After First Save
  mov byte ptr [chpstage],05
  cmp bx,4D28
  je spawn
  mov byte ptr [chpstage],06
  cmp bx,FC20
  je spawn
  mov byte ptr [chpstage],00
  //After first live of a thousand gods
  cmp bx,7680
  je greyninja
  cmp bx,8640
  je greyninja
  //Chest with IS
  cmp bx,B388
  je setenemychp0105
  cmp bx,C348
  je setenemychp0105
  //First Ledge Run
  cmp bx,3928
  je setenemychp0106
  //Second Ledge Run
  cmp bx,7630
  je setenemychp0107
  cmp bx,85F0
  je greyninjabow
  //Delimbed Greys Before Save
  mov byte ptr [chpstage],07
  cmp bx,F090
  je spawn
  cmp bx,0C20
  je spawn
  cmp bx,27B0
  je mage
  mov byte ptr [chpstage],00
  //Wall Jump First Battle After Save
  cmp bx,13D0
  je setenemychp0109
  //Water Battle
  cmp bx,5108
  je setenemychp01010
  //Before Buddah Cutscene
  cmp bx,7670
  je setenemychp01018
  //After Cutscene (2 melee grey)
  mov byte ptr [chpstage],05
  cmp bx,52D0
  je spawn
  mov byte ptr [chpstage],00
  //Archer
  cmp bx,8FD8
  je greyninjabow
  mov byte ptr [chpstage],06
  //First Ledge Jump
  cmp bx,BD10
  je spawn
  //Second Ledge Jump
  cmp bx,E278
  je spawn
  mov byte ptr [chpstage],00
  //Drop Down
  cmp bx,D2C8
  je isninja{setenemychp01011}
  //BUDDAH FIGHTER ARENA
  cmp bx,0DF8
  je setenemychp01020
  cmp bx,1DB8
  je setenemychp01019
  cmp bx,2988
  je codea
  cmp bx,3948
  je codea
  //BRIDGE
  mov byte ptr [chpstage],05
  cmp bx,9BD8
  je spawn
  //TEMPLE
  cmp bx,8DF0
  je spawn
  cmp bx,A980
  je mage
  {cmp bx,9DB0
  je greyninja}
  cmp bx,D6C8
  je spawn
  cmp bx,E688
  je spawn
  mov byte ptr [chpstage],00
  //Muramasa
  cmp bx,52F0
  je isninja
  cmp bx,62B0
  je isninja
  {//Bats
  cmp bx,13F0
  je setenemychp01012}
  mov byte ptr [chpstage],05
  //Billboard Run
  cmp bx,7C48
  je setenemychp0106
  //Before Liz Cutscene
  cmp bx,B560//Grey
  je spawn
  mov byte ptr [minimumspawn],#2
  mov byte ptr [maximumspawn],#3
  mov byte ptr [chpstage],07
  cmp bx,D0F0//IS
  je setenemychp0109
  mov byte ptr [chpstage],00
  mov byte ptr [minimumspawn],#1
  mov byte ptr [maximumspawn],#2
  cmp bx,C520//Grey
  je spawn
  mov byte ptr [minimumspawn],#2
  mov byte ptr [maximumspawn],#3
  mov byte ptr [chpstage],07
  cmp bx,E0B0//IS
  je spawn
  cmp bx,1B90
  je spawn
   mov byte ptr [chpstage],00
  mov byte ptr [minimumspawn],#1
  mov byte ptr [maximumspawn],#2
  //After Cutscene
  {cmp bx,1B90
  je spawn}
  //Stair Climb
  cmp bx,44E8
  je setenemychp01022
  cmp bx,54A8
  je setenemychp0109
  //Wall jump
  cmp bx,83E8
  je setenemychp01015
  //Last Battle
  cmp bx,C2E8
  je setenemychp01016
  cmp bx,D2A8
  je setenemychp01016
  cmp bx,E460
  je setenemychp01017
