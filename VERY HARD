registersymbol(darkTable)

darkTable:
dd BDB0//START
dd 5528//BRIDGE NEXT TO CLAW ROOM
dd 4D48//CLAW ROOM
dd ECF0//BEFORE FIRST SAVE (CORRIDOR)
dd BFA8//FIRST SAVE
dd 3D88//FIRST BRIDGE
dd 4178//FIRST BRIDGE 8C48
dd 9428//SAVE ONTOP OF BRIDGE
dd 27E0//SAVE ONTOP OF BRIDGE
{dd 35A8//JUMP UP FROM MINE - CAMERA ZOOM}
dd 9C08//JUMP UP FROM MINE
{dd 8C48}//MINE START - KEEP OFF
dd ABC8//WOOD BRIDGE
dd B3A8//WOOD BRIDGE
dd AFB8//WOOD BRIDGE END
dd 5D08//MAGE BATTLE ON ROOF BEFORE GENSHIN
dd 74A8//ARCHER ROOM ENTER
dd B798//WOOD BRIDGE

newmem82:
 push rax
 push rbx
 push rcx
 push rdx
 push rsi
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#07
 jne triggerStart
 //CHP2 LIGHT TRIGGERS
 lea rdx,[darkTable]
 xor rcx,rcx
darkStart:
 mov rsi,[rdx+rcx*4]
 cmp si,bx
 je setDark
 inc rcx
 cmp [rdx+rcx*4],00
 jnz darkStart
triggerStart:
 cmp byte ptr [canDelay],01
 je delayTrigger
 cmp byte ptr [canDelay],00
 je code82
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2FDA168],FF
 je code82
 movzx rax, byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2FDA168]
 cmp al,[delayThreshold]
 jle delayFinish
 mov rbx, [triggerAddress]
 mov [rbx],03
 nop
 nop
 nop
 nop
 nop
 pop rsi
 pop rdx
 pop rcx
 pop rbx
 pop rax
 jmp return82
delayFinish:
 mov byte ptr [canDelay],00
 mov rbx, [triggerAddress]
 cmp rbx,0000000000000000
 je code82
 xor rax,rax
 mov [rbx],01
 mov [triggerAddress],rax
 jmp code82
delayTrigger:
 mov byte ptr [canDelay],02
 cmp [triggerDelayAmt],(float)0.00
 jle code82
 mov byte ptr [canDelay],01
 mov [tDec],(float)1.00
 movss xmm0,[triggerDelayAmt]
 movss xmm1,[tDec]
 subss xmm0,xmm1
 movss [triggerDelayAmt],xmm0
 jmp code82
setDark:
 mov [rbx],02
code82:
  pop rsi
  pop rdx
  pop rcx
  pop rbx
  pop rax
  call "NINJA GAIDEN SIGMA2.exe"+1607590
  jmp return82

TRIGGERDELAY:
  jmp newmem82
return82:
registersymbol(TRIGGERDELAY)

