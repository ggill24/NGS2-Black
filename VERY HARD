  mov byte ptr [canupgrade],#0
  pop rax
code12:
  mov ["NINJA GAIDEN SIGMA2.exe"+215BB94],00000005
  jmp return12

ESSREMOVAL:
  jmp newmem12
  nop 5
return12:
