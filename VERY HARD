eweapon:
 cmp cx,69 //GUIL THROW
 je guilthrow
 cmp di,322//GREY/TAC FS HIT IN AIR
 je fs
 cmp di,231//GREY/TAC FS HIT FLOOR
 je fsfloor
 cmp di,1E3//HIT FLOOR
 je fsfloor
 cmp di,246//IS FS HIT IN AIR
 je fs
 cmp di,110//IS FS HIT ON FlOOR
 je fsfloor
 cmp di,15E//IS FS HIT ON FlOOR
 je fsfloor
 cmp di,250//IS FIEND FS HIT IN AIR
 je fs
 cmp di,118//IS FIEND FS HIT ON FLOOR
 je fsfloor
 cmp di,25A//Grey Fiend FS HIT IN AIR
 je fs
 cmp di,11C//Grey Fiend FS HIT Floor
 je fsfloor
 cmp di,1F5//Grey Mage FS HIT IN AIR
 je fs
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CBE6],007
 je tds
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CBE6],001
 je ds
