registersymbol(hpKill)

hpKillEntTable:
db 09C
db 070
db 038
db 09D
db 07C
db 02F
db 0A1
db 003
db 0A7
db 0B1
db 006
db 09E
db 00C
db 07F
db 01C
db 07A
db 0F1

hpKillMoveTable:
dd 0DC7//KG DOWNWARD FLYING SWALLOW
dd 01BA//AYANE RUN IS
dd 01A6//AYANE STAND IS

newmem6:
   push rax//ENT ARRAY
   push rcx//ENT COUNTER
   push rdx//MOVE COUNTER
   push rsi//ENT
   push rdi//MOVE
   push rbp//PLAYER CURRENT MOVE / ENT HIT
   push r9 //MOVE ARRAY
   cmp byte ptr [disablehpreduction],01
   jae code6
   cmp qword ptr [enthitadd],rbx
   jne code6
   mov byte ptr [hpKill],00
   cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0C7C//DS FS
   je hpfs
   cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0C73//DONT REMEMBER L
   je hpfs
   cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0D6E//BoTA FS
   je hpfs
   lea rax,[hpKillEntTable]
   lea r9,[hpKillMoveTable]
   xor rcx,rcx
   xor rdx,rdx
hpKillMoveStart:
   mov rdi,[r9+rdx*4]
   mov rbp,["NINJA GAIDEN SIGMA2.EXE"+319CC30]
   cmp bp,di
   je hpKillEntStart
   inc rdx
   cmp word ptr [r9+rdx*4],00
   jnz hpKillMoveStart
   jmp code6//NO POINT IN LOOPING ENT LIST IF PLAYER MOVE IS NOT IN MOVE TABLE
hpKillEntStart:
   movzx rsi, byte ptr [rax+rcx]
   movzx rbp, byte ptr [rbx+18]
   mov byte ptr [hpKill],01
   cmp si,bp
   je code6
   mov byte ptr [hpKill],00
   inc rcx
   cmp byte ptr [rax+rcx],00
   jnz hpKillEntStart
   jmp code6
hpfs:
  mov byte ptr [hpKill],00
  cmp byte ptr [rsi+5B30+015D],#16
  jne code6
  mov byte ptr [hpKill],01
  jmp code6
code6:
  pop r9
  pop rbp
  pop rdi
  pop rsi
  pop rdx
  pop rcx
  pop rax
  cmp byte ptr [hpKill],01
  jne hpEnd
  mov byte ptr [hpKill],00
  {cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CBE6],04A
  je hpAyane}
  xor ax,ax
  jmp hpEnd
hpAyane:
  xor ax,ax
  add ax,0A
hpEnd:
  mov [rsi+00005AB6],ax
  jmp return6

HPMOD+03:
  jmp newmem6
  nop 2
return6:
registersymbol(HPMOD)

