aobscanmodule(iFrame,NINJA GAIDEN SIGMA2.exe,C6 80 58 5B 00 00 00 48)
alloc(newmem50,$100,iFrame)

label(code50)
label(return50)
alloc(iFrameTable,16)
registersymbol(iFrameTable)

iFrameTable:
//KG
dd 08C7
dd 08C8
dd 0894
dd 0895

newmem50:
  push rbx
  push rcx
  push rdx
  push rsi
  mov byte ptr [rax+00005B58],00
  cmp ax,7100//PLAYER
  jne iFrameCleanUp
  cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CBB4],00//DEAD
  je iFrameCleanUp
  lea rsi,[iFrameTable]
  xor rcx,rcx
iFrameStart:
  mov rbx,["NINJA GAIDEN SIGMA2.EXE"+319CC30]
  mov rdx,[rsi+rcx*4]
  cmp bx,dx
  mov byte ptr [rax+00005B58],01//SET IFRAME
  je iFrameCleanUp
  mov byte ptr [rax+00005B58],00
  inc rcx
  cmp [rsi+rcx*4],00
  jnz iFrameStart
iFrameCleanUp:
  pop rsi
  pop rdx
  pop rcx
  pop rbx
code50:
  jmp return50

iFrame:
  jmp newmem50
  nop 2
return50:
registersymbol(iFrame)

