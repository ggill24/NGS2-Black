newmem61:
  push rbx
  xor rbx,rbx
  lea rbx,["NINJA GAIDEN SIGMA2.EXE"+2F78B10]
  cmp rbx,rsi
  je code61
  xor rbx,rbx
  lea rbx,[rdx+38]
  mov rbx, qword ptr [rbx]
  cmp rbx,65535
  jl code61
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2FDA168],02
  jle code61
  {cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#12
  je isSpam
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#15
  je isSpam
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#16
  je isSpam}
  cmp byte ptr [enableIS],01
  jne code61
