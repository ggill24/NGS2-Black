 mov byte ptr [ras],0
 jmp coded
buddah:
 mov dil,01
coded:
  mov ["NINJA GAIDEN SIGMA2.exe"+1F5070A],dil
  jmp returnd

DESPAWN:
  jmp newmemd
  nop 2
returnd:
