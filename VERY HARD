newmem:
  mov byte ptr [phitent],00
  mov qword ptr [enthitadd],FFFFFFFFFFFFFFFF
  mov byte ptr [ent],FF
  cmp bx,5300
  je code
  mov byte ptr [phitent],01
  cmp byte ptr [disabledelimb],01
  jae code
  mov [enthitadd],rbx
  mov [enthitadd2],rbx
  mov [ent],cx
setdelimbpercent:
 push rax
 push rbx
 push rcx
 push rdx
 push rsi
 push rdi
 push rbp
 push r8
 push r9
 push r10
 push r11
 push r12
 push r13
 push r14
 push r15
 call rand
 xor rdx,rdx
 mov rcx, #100
 div rcx
 mov [delimbp],dx
 pop r15
 pop r14
 pop r13
 pop r12
 pop r11
 pop r10
 pop r9
 pop r8
 pop rbp
 pop rdi
 pop rsi
 pop rdx
 pop rcx
 pop rbx
 pop rax
 {cmp r15w,68
 jne code
 mov qword ptr [enthitadd2],FFFFFFFFFFFFFFFF}
code:
  cmp edx,000000A0
  jmp return
DELIMB1:
  jmp newmem
  nop
return:
registersymbol(DELIMB1)

