  je animIS
  cmp byte ptr [rbx+18],070
  je animISP
  jmp code61
animIS:
   xor rbx,rbx
   mov bx,ax
   cmp bx,024
   mov ax,08B
   je code61
   mov ax,08A
   cmp bx,026
   je isCoolCheck
   mov ax,bx
   jmp code61
