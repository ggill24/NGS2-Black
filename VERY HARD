label(code15)
label(return15)
alloc(racemulp,02)
registersymbol(racemulp)
newmem15:
  mov [racemulp],(float)3.00
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#11
  je setninjainitialtimer
  mov [racemulp],(float)1.50
setninjainitialtimer:
  xorps xmm1,xmm1
  movss  xmm1,[racemulp]
  mulps xmm0,xmm1
  xorps xmm1,xmm1
code15:
  cvttss2si r11,xmm0
  jmp return15

NINJARACESTARTTIMER:
  jmp newmem15
return15:
registersymbol(NINJARACESTARTTIMER)

