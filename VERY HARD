  cmp byte ptr [rax+5C03],01
  je code2
  cmp byte ptr [rax+5B30+015D],#16
  je code2
  cmp byte ptr [rbx+18],09E
  je bigentrightlegaircheck
  cmp byte ptr [rbx+18],0A7
  je bigentrightlegaircheck
  cmp byte ptr [rbx+18],003
  je bigentrightlegaircheck
  cmp byte ptr [rbx+18],00C
  je bigentrightlegaircheck
  mov cx,9A
  mov di, word ptr [rightleg]
  cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],094A
  je code2
  cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0419
  je code2
  cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],069C
  je code2
  cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0728
  je code2
