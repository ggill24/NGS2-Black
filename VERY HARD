newmem2:
  push r9
  cmp byte ptr [disabledelimb],01
  jae code2
  cmp ax,7100
  je isPlayer
  cmp byte ptr [rax+5B30+015D],#16
  je leggroundcheck
  cmp qword ptr [enthitadd],rbx
  je start
  push rax
  movzx rax, byte ptr [rbx+18]
  mov [enothit],al
  pop rax
  jmp code2
isPlayer:
  mov [plranim],di
  {cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2DBCB50],01
  je sufferRyu
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2E29F94],01
  je sufferRyu
  cmp byte ptr [nindance],01
  je sufferRyu
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2DBCB50],02
  je ryuWake
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2E29F94],02
  je ryuWake
  cmp byte ptr [nindance],02
  je ryuWake}
  jmp code2
sufferRyu:
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#19
 je ryuFall
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#16
 je ryuFall
 jmp code2
ryuFall:
 mov cx,FF
 mov di,A82
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CD04],01
 je setRyuFall
 mov di,AFE
setRyuFall:
 mov byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2DBCB50],02
 mov byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2E29F94],02
 mov byte ptr [nindance],02
 mov ["NINJA GAIDEN SIGMA2.EXE"+319CBDA],0
 jmp code2
ryuWake:
 cmp di,4D
 jne code2
 mov cx,FF
 mov di,A75
 mov byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2DBCB50],03
 mov byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2E29F94],03
 mov byte ptr [nindance],00
 jmp code2
leggroundcheck:
  mov word ptr [entleggroundanim],03D7
  cmp byte ptr [rbx+18],09D
  je setlegground
  mov word ptr [entleggroundanim],03D7
  cmp byte ptr [rbx+18],038
  je setlegground
  mov word ptr [entleggroundanim],0301
  cmp byte ptr [rbx+18],02F
  je setlegground
  mov word ptr [entleggroundanim],02ED
  cmp byte ptr [rbx+18],09C
  je setlegground
  mov word ptr [entleggroundanim],0303
  cmp byte ptr [rbx+18],070
  je setlegground
  {mov word ptr [entleggroundanim],0093
  cmp byte ptr [rbx+18],00C
  je setlegground}
  mov word ptr [entleggroundanim],0000
  jmp code2
setlegground:
  cmp di,6D
  jl fixleggronudanim
  cmp qword ptr [enthitadd],rbx
  je start
  jmp code2
fixleggronudanim:
  mov di,word ptr [entleggroundanim]
  mov cx,9A
  jmp code2
start:
 mov [state],cx
 cmp byte ptr [ent],09D //GREY NINJA
 je setGreyNinja
 cmp byte ptr[ent],02F //GREYNINJAFIEND
 je setgreyninjafiend
 cmp byte ptr [ent],09C //GREY NINJA
 je setISNinja
 cmp byte ptr[ent],070 //ISNINJAFIEND
 je setisninjafiend
 cmp byte ptr[ent],038 //TACNINJA
 je settacninja
 cmp byte ptr[ent],003 //VANGELF PURPLE/BLACK
 je setvangelf
 cmp byte ptr[ent],0A7 //VANGELF GOLF
 je setvangelfGOLD
 cmp byte ptr[ent],006 //WOLF GREY
 je setwolf
 cmp byte ptr[ent],0B1 //WOLF RED
 je setwolf
 cmp byte ptr[ent],09E //RED GAJA
 je setgaja
 cmp byte ptr[ent],032 //PURPLE GAJA
 je setgaja
 cmp byte ptr[ent],0A1 //MAGE
 je setmage
 cmp byte ptr[ent],07C //MAGEFIEND
 je setmage
 {cmp byte ptr[ent],0F1 //IMPS
 je setimps}
 cmp byte ptr[ent],00C //Chainsaw zombie
 je setzombie
 cmp byte ptr[ent],0F9 //Buddah Statue (second form)
 je buddahKill
 jmp code2
buddahKill:
  cmp cx,00
  je code2
  mov di,055
  jmp code2
