aobscanmodule(INJECTc,NINJA GAIDEN SIGMA2.exe,FF C0 83 F8 07)
alloc(newmemc,$1000,INJECTc)

label(codec)
label(returnc)
newmemc:
  cmp byte ptr [disablespawns],01
  jae codec
  cmp byte ptr [canspawn],0
  jle codec
  cmp byte ptr [onstart],-01
  jne clear
  mov eax,02
  jmp codec
clear:
 mov byte ptr [onstart],-01
 jmp codec
codec:
  //0 Enemies Alive
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2FDA168],00
  je resetaggro
  inc eax
  cmp eax,07
  jmp returnc
resetaggro:
  mov [aggrotimer],(float)0.00
  mov byte ptr [canaggro],00
  mov byte ptr [phitent],00
  inc eax
  cmp eax,07
  jmp returnc
INJECTc:
  jmp newmemc
returnc:
registersymbol(INJECTc)

aobscanmodule(isDead,NINJA GAIDEN SIGMA2.exe,66 83 BA B6 5A 00 00 00 74 1F)
alloc(newmem41,$100,isDead)

label(code41)
label(return41)

newmem41:
  cmp word ptr [rdx+00005AB6],00
  je pDead
  jmp code41
pDead:
   mov byte ptr [ras],00
   mov byte ptr [chapter14tengu],00
   mov byte ptr [counter],00
   mov byte ptr [canspawn],00
   mov byte ptr [chpstage],00
   mov byte ptr [ninpo],00
   mov byte ptr [scounter],00
   mov byte ptr [bcounter],00
   mov byte ptr [canaggro],01
   mov [aggrotimer],(float)0.00
   mov [cBypassTime],(float)0.00
   mov byte ptr [isUnlocked],00
   mov byte ptr [phitent],00
   mov qword ptr [enthitadd],FFFFFFFFFFFFFFFF
   mov qword ptr [enthitadd2],FFFFFFFFFFFFFFFF
   mov byte ptr [ent],FF
   mov byte ptr [phitent],00
   mov byte ptr [nindance],00
   mov byte ptr [weakAlexi],00
   mov byte ptr [canClip],00
   mov byte ptr [wDOT],00
   mov byte ptr [canDelay],00
   mov byte ptr [sWave],00
   mov byte ptr [delayThreshold],00
   mov ["NINJA GAIDEN SIGMA2.EXE"+6BED024],(float)0.020
   mov ["NINJA GAIDEN SIGMA2.EXE"+6BED028],(float)1.10
   cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#06
   jae resetspawnstage
   jmp code41
resetspawnstage:
 mov byte ptr [chpstage],00
code41:
  cmp word ptr [rdx+00005AB6],00
  jmp return41
isDead:
  jmp newmem41
  nop 3
return41:
registersymbol(isDead)

