 xorps xmm5,xmm5
 cvtsi2ss xmm0,r11
 xor r11,r11
 mov r11,354
setninjatimer:
 cvtsi2ss xmm5,r11
 addss xmm0,xmm5
 cvtss2si r11,xmm0
