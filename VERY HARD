aobscanmodule(AIGGRO3,NINJA GAIDEN SIGMA2.exe,B9 A0 00 00 00 66 3B C8 76 53)
alloc(newmem25,$1000,AIGGRO3)

label(code25)
label(return25)
alloc(aggrotimer,8)
registersymbol(aggrotimer)
alloc(decv,2)
registersymbol(decv)

newmem25:
  cmp byte ptr [disableai],01
  jae code25
  cmp [aggrotimer],(float)0.00
  ja timer
  {cmp byte ptr [aient],006
  je entchecked
  cmp byte ptr [aient],0B1
  je entchecked
  {cmp byte ptr [aient],0A1
  je entchecked}
  cmp byte ptr [aient],07F
  je entchecked
  cmp byte ptr [aient],09D
  je entchecked
  {cmp byte ptr [aient],07C
  je entchecked}
  cmp byte ptr [aient],02F
  je entchecked
  cmp byte ptr [aient],09E
  je entchecked
  cmp byte ptr [aient],0F1
  je entchecked
  cmp byte ptr [aient],038
  je entchecked
  {cmp byte ptr [aient],0BB
  je entchecked}
  {cmp byte ptr [NG2AI],01
  jae ignoreai}
  cmp byte ptr [aient],070
  je entchecked
  cmp byte ptr [aient],09C
  je entchecked
  mov byte ptr [aient],00
  jmp resetai
entchecked:
  //AGGRO TIMER IS IN EFFECT
  cmp [aggrotimer],(float)0.00
  ja timer
  //PLAYER IN AIR
  mov [aggrotimer],(float)2000.00
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CD04],01
  je setcooldown
  //PLAYER HIT
  mov [aggrotimer],(float)1200.00
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CD05],02
  je setcooldown
  //PLAYER IN AN IFRAME STATE (UT,OT,NINPO ETC..)
  mov [aggrotimer],(float)900.00
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC58],01
  je setcooldown
  //PLAYER Not in Attack/Attacking/Hit State
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CD05],00
  je aggression
  //PLAYER HIT ENEMY
  mov [aggrotimer],(float)2700.00
  cmp byte ptr [phitent],01
  je setcooldown
aggression:
 mov byte ptr [canaggro],01
 mov [aggrotimer], (float)0.00
 xor rax,rax
 jmp return25
setcooldown:
  mov byte ptr [canaggro],00
  push rbx
  xor rbx,rbx
  movzx rax,byte ptr ["NINJA GAIDEN SIGMA2.EXE"+2FDA168]
  cvtsi2ss xmm0,rax
  movss xmm1,[aggrotimer]
  mulss xmm1,xmm0
  movss [aggrotimer],xmm1
  xorps xmm0,xmm0
  xorps xmm1,xmm1
  pop rbx
timer:
  mov [decv],(float)1.00
  movss xmm0,[decv]
  movss xmm1,[aggrotimer]
  subss xmm1,xmm0
  movss [aggrotimer],xmm1
  cmp [aggrotimer],(float)0.00
  jle aggression
  jmp code25
resetai:
 mov [aggrotimer],(float)0.00
code25:
  mov byte ptr [canaggro],00
  mov byte ptr [aient],00
  mov ecx,000000A0
  jmp return25
AIGGRO3:
  jmp newmem25
return25:
registersymbol(AIGGRO3)

