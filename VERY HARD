aobscanmodule(WEAPONUPGRADE,NINJA GAIDEN SIGMA2.exe,0F 8E A5 01 00 00 FF)
alloc(newmem11,$100,WEAPONUPGRADE)

label(code11)
label(return11)

newmem11:
  mov byte ptr [canupgrade],#0
  mov dword [esscost],#15000
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C4218],#01 //ACOLYTE
  je reqcheck
  mov dword [esscost],#20000
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C4218],#02 //WARRIOR
  je reqcheck
  mov dword [esscost],#25000
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C4218],#03 //MENTOR
  je reqcheck
  mov dword [esscost],#30000
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C4218],#04 //MASTER NINJA
  je reqcheck
  mov dword [esscost],#30000//DEFAULT
reqcheck:
  push rax
  mov rax, dword ptr [esscost]
  cmp ["NINJA GAIDEN SIGMA2.exe"+6795B80],rax
  jae allowupgrade
  pop rax
  mov byte ptr [canupgrade],#0
  jl "NINJA GAIDEN SIGMA2.exe"+13D2DAD
  jmp return11
allowupgrade:
 pop rax
 mov byte ptr [canupgrade],#1
 jmp return11
code11:
  jng "NINJA GAIDEN SIGMA2.exe"+13D2DAD
  jmp return11

WEAPONUPGRADE:
  jmp newmem11
  nop
return11:
registersymbol(WEAPONUPGRADE)

