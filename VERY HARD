1 = Staggered leg delimb drop
0 = Quick leg delimb drop
}


tds:
 cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0C7A//FS
 je fs
 cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0C7C//FS
 je fs
 mov byte ptr [legvariant],00
 mov byte ptr [limbc],04
 cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],094E //XXY->Y
 je delimbconditions
 mov byte ptr [limbc],28
 cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0417 //->X
 je delimbconditions
 mov byte ptr [limbc],31
 cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0405 //OL X
 je delimbconditions
 cmp byte ptr [rax+5C05],0 //0 = NEUTRAL STATE 1 = ATTACK MODE 2 = HIT
 je code2
 mov byte ptr [limbc],15
 cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CC30],0944 //YYY
 je delimbconditions
