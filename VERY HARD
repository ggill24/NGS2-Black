 je fiendGenshin
 cmp byte ptr [rbx+18],012
 je liz
 cmp byte ptr [rbx+18],023
 je pRas
 cmp byte ptr [rbx+18],0A5
 je pRas
 cmp byte ptr [rbx+18],039
 je waterDragon
 jmp code13
waterDragon:
 cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+31957B0+D0B6],028A
 jle waterDragonOT
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CD03],04//RYU SURFACE WATER
 jnae code13
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CD03],05//RYU SUBMERGED
 ja code13
 mov dx,04//MOVE AROUND
 cmp r8w,12//FAKE OUT
 je waterDragonSwim
 {cmp r8w,10//SCREAM
 je waterDragonSwim}
 mov dx,1D//UNDER WATER GRAB
 cmp r8w,0F//Call for the homies
 je waterDragonGrab
 mov dx,1D//UNDER WATER GRAB
 cmp r8w,10//YELL
 je waterDragonGrab
 mov dx,1E//WATER SPLASH
 cmp r8w,014//WHIPLASH
 je waterDragonSplash
 mov dx,1E//WATER SPLASH
 cmp r8w,016//Spikes
 je waterDragonSplash
 mov dx,1E//WATER SPLASH
 cmp r8w,023//YELL
 je waterDragonSplash
 mov dx,r8w
 jmp code13
