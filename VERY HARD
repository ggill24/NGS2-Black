  cmp rax,43D
  je S1L1Y
  cmp rax,01D
  je S1L1Y
  cmp rax,46D
  je S1UT
  cmp rax,936
  je S1Kick
  cmp rax,93D
  je S1UT2
  {cmp rax,407
  mov rdx,938
  je code13
  cmp rax,936
  je S1Kick
  {mov rdx,420}
  {je code13
  cmp rax,46D
  mov rdx,45D
  je code13
  mov rdx,r8
  cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+319CBE6],01 //DS - TOO OP FOR NON TDS
  je code13
  cmp rax,93D
  mov rdx,45F
  je code13
  mov rdx,r8}
  jmp code13
NG1Counter2:
