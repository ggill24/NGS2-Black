aobscanmodule(RESET,NINJA GAIDEN SIGMA2.exe,49 8B C8 F3 AA)
alloc(newmemb,$1000,RESET)

label(codeb)
label(returnb)

newmemb:
 push rax
 xor rax,rax
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#123
 jae codeb
 cmp byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A],#6
 jl resetcounters
 cmp word ptr ["NINJA GAIDEN SIGMA2.EXE"+319CBB4],#0
 jle resetcounters
 jmp codeb
resetcounters:
 mov [maximumspawn],al
 mov [minimumspawn],al
 mov [spawnlimit],al
 mov [currentlevel],al
 mov [chpstage],al
 mov [ras],al
 mov [counter],al
 mov [ninpo],al
 mov [isUnlocked],al
 mov [canDelay],al
 mov [sWave],al
 mov [delayThreshold],al
 mov [canaggro],al
 mov [phitent],al
 mov [scounter],al
 mov [nindance],al
 mov [weakAlexi],al
 mov [canClip],al
 mov [wDOT],al
 mov ["NINJA GAIDEN SIGMA2.EXE"+6BED024],(float)0.020
 mov ["NINJA GAIDEN SIGMA2.EXE"+6BED028],(float)1.10
 mov [cBypassTime],(float)0.00
 pop rax
 mov rcx,r8
 repe stosb
 jmp returnb
codeb:
 movzx rax,byte ptr ["NINJA GAIDEN SIGMA2.EXE"+35C421A]
 mov [currentlevel],al
 pop rax
 mov rcx,r8
 repe stosb
 jmp returnb

RESET:
  jmp newmemb
returnb:
registersymbol(RESET)


aobscanmodule(NINJARACESTARTTIMER,NINJA GAIDEN SIGMA2.exe,F3 4C 0F 2C D8 0F 57)
