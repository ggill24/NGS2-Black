 mov r14w,09E
 cmp byte ptr [bcounter],1 //ZED REMOVED
 jae coded
 inc byte ptr [bcounter]
 mov r14w,024
 jmp coded
volfcheck:
 mov r14w,072
 cmp byte ptr [bcounter],01
 jae coded
 inc byte ptr [bcounter]
 mov r14w,025
 jmp coded
removeentity:
